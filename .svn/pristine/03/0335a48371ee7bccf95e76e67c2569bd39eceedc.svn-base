using System;

namespace  CWI.MCP.Common
{
    /// <summary>
    /// 客户端信息
    /// </summary>
    [Serializable]
    public class RequestClientInfoModel
    {
        /// <summary>
        /// 应用标识：EPT-云打印
        /// </summary>
        public string AppSign { get; set; }

        /// <summary>
        /// AppVersion	如：1.0	应用的版本号
        /// </summary>
        public string AppVersion { get; set; }

        /// <summary>
        /// 设备类型:0-其它,1-Android,2-IOS
        public string DeviceType { get; set; }

        /// <summary>
        /// MAC地址
        /// </summary>
        public string DeviceId { get; set; }

        /// <summary>
        /// 客户端IP地址
        /// </summary>
        public string ClientIP { get; set; }

        /// <summary>
        /// 客户端推送Token
        /// </summary>
        public string ClientToken { get; set; }
    }
}
